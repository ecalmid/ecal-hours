        "pouchdb-find": "^7.2.1",
        "rrule": "^2.6.0",
        "vue": "^2.6.6",
        "vue-contenteditable-directive": "^1.2.0",
        "vue-meta": "^2.3.3",
        "vuex": "^3.1.3"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.5.0",
        "@vue/cli-plugin-eslint": "^3.5.0",
        "@vue/cli-service": "^3.8.0",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.0.1",
        "dotenv": "^7.0.0",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0",
        "ftp-deploy": "^2.3.3",
        "lint-staged": "^8.1.4",
        "node-sass": "^7.0.0",
        "sass-loader": "^7.1.0",
        "vue-cli-plugin-pug": "^1.0.7",
        "vue-template-compiler": "^2.5.21"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/standard"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,vue}": [
            "vue-cli-service lint",
            "git add"
        ]
    }
