{
    "name": "hours",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "deploy": "npm run build && node deploy"
    },
    "dependencies": {
        "axios": ">=0.19.0",
        "ics-to-json": "^1.0.0",
        "moment": "^2.24.0",
